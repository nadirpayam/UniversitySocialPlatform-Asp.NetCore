@model IEnumerable<EntityLayer.Concrete.Section>


<div class="tech-btm">
    <h4>Bölümler</h4>
    <ul class="list-group single">
        @foreach (var item in Model)
        {
            
            <li class="list-group-item d-flex justify-content-between align-items-center">
                <a href="/Post/ListBySectionId/@item.SectionID">
                    @item.SectionName
                </a>
            </li>
        }
    </ul>
</div>