@using UniversitySocialPlatform.Controllers
@model List<Class1>


@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>Index</h1>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Adı Soyadı</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
            <td><a href="/EmployeeTest/DeleteEmployee/@item.ID"" class="btn btn-danger">Sil</a></td>
            <td><a href="/EmployeeTest/EditEmployee/@item.ID" class="btn btn-warning">Düzenle</a></td>
        </tr>
    }
</table>
<a href="/EmployeeTest/AddEmpoyee/" class="btn btn-outline-primary">Yeni Çalışan Ekle</a>